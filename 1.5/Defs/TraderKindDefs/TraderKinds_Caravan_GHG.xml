<?xml version="1.0" encoding="utf-8"?>
<Defs>

<TraderKindDef><!-- 貿易商 -->
	<defName>Caravan_GHG_BulkGoods</defName>
	<label>bulk goods trader</label>
	<stockGenerators>

	<li Class="StockGenerator_SingleDef">
		<thingDef>Silver</thingDef>
		<countRange>750~1200</countRange>
	</li>
	<li Class="StockGenerator_SingleDef">
		<thingDef>WoodLog</thingDef>
		<countRange>100~350</countRange>
	</li>
	<li Class="StockGenerator_SingleDef">
		<thingDef>Neutroamine</thingDef>
		<countRange>30~50</countRange>
	</li>
	<li Class="StockGenerator_SingleDef">
		<thingDef>Gold</thingDef>
		<countRange>10~350</countRange>
	</li>
	<li Class="StockGenerator_SingleDef">
		<thingDef>Steel</thingDef>
		<countRange>250~300</countRange>
	</li>
	<li Class="StockGenerator_SingleDef">
		<thingDef>Uranium</thingDef>
		<countRange>150~250</countRange>
	</li>
	<li Class="StockGenerator_SingleDef">
		<thingDef>MedicineHerbal</thingDef>
		<countRange>0~10</countRange>
	</li>
	<li Class="StockGenerator_Category">
		<categoryDef>Textiles</categoryDef>
		<thingDefCountRange>1~2</thingDefCountRange>
		<totalPriceRange>300~700</totalPriceRange>
	</li>
	<li Class="StockGenerator_SingleDef">
		<thingDef>Dye</thingDef>
		<countRange>0~20</countRange>
	</li>
	<li Class="StockGenerator_SingleDef">
		<thingDef>Beer</thingDef>
		<totalPriceRange>100~300</totalPriceRange>
	</li>
	<li Class="StockGenerator_Category">
		<categoryDef>FoodRaw</categoryDef>
		<excludedCategories><li>EggsFertilized</li></excludedCategories>
		<thingDefCountRange>4~4</thingDefCountRange>
		<totalPriceRange>400~600</totalPriceRange>
	</li>
	<li Class="StockGenerator_MultiDef">
		<thingDefs>
			<li>Chocolate</li>
			<li>Kibble</li>
		</thingDefs>
	</li>

<!-- Drugs -->
	<li Class="StockGenerator_Category">
		<categoryDef>Drugs</categoryDef>
		<maxTechLevelGenerate>Medieval</maxTechLevelGenerate>
	</li>

<!-- Apparel -->
	<li Class="StockGenerator_MarketValue">
		<tradeTag>BasicClothing</tradeTag>
		<countRange>3~5</countRange>
		<maxTechLevelGenerate>Medieval</maxTechLevelGenerate>
	</li>
	<li Class="StockGenerator_MarketValue">
		<tradeTag>Clothing</tradeTag>
		<countRange>1~3</countRange>
		<maxTechLevelGenerate>Medieval</maxTechLevelGenerate>
	</li>

<!-- Buying -->
	<li Class="StockGenerator_BuyExpensiveSimple" />
	<li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
	<li Class="StockGenerator_BuyTradeTag"><tag>MusicalInstrument</tag></li>
	<li Class="StockGenerator_BuyTradeTag"><tag>GHG_Apparel</tag></li>

	</stockGenerators>
</TraderKindDef>

<TraderKindDef><!-- 酒屋 -->
	<defName>Caravan_GHG_SakeDealer</defName>
	<label>sake dealer</label>
	<stockGenerators>

	<li Class="StockGenerator_SingleDef">
		<thingDef>Silver</thingDef>
		<countRange>750~1200</countRange>
	</li>
	<li Class="StockGenerator_SingleDef">
		<thingDef>MedicineHerbal</thingDef>
		<countRange>30~50</countRange>
	</li>
	<li Class="StockGenerator_SingleDef">
		<thingDef>Neutroamine</thingDef>
		<countRange>10~30</countRange>
	</li>
	<li Class="StockGenerator_SingleDef">
		<thingDef>Beer</thingDef>
		<countRange>70~100</countRange>
	</li>
	<li Class="StockGenerator_Category">
		<categoryDef>Drugs</categoryDef>
		<thingDefCountRange>3~5</thingDefCountRange>
		<totalPriceRange>300~800</totalPriceRange>
	</li>
	<li Class="StockGenerator_Category">
		<categoryDef>FoodMeals</categoryDef>
		<thingDefCountRange>3~30</thingDefCountRange>
		<countRange>15~30</countRange>
	</li>
	<li Class="StockGenerator_MultiDef">
		<thingDefs>
			<li>Chocolate</li>
			<li>Kibble</li>
		</thingDefs>
	</li>

<!-- Buying -->
	<li Class="StockGenerator_BuyExpensiveSimple" />
	<li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
	<li Class="StockGenerator_BuyTradeTag"><tag>MusicalInstrument</tag></li>
	<li Class="StockGenerator_BuyTradeTag"><tag>GHG_Apparel</tag></li>

	</stockGenerators>
</TraderKindDef>

<TraderKindDef><!-- 家畜商 -->
	<defName>Caravan_GHG_Drover</defName>
	<label>livestocks trader</label>
	<stockGenerators>

	<li Class="StockGenerator_SingleDef">
		<thingDef>Silver</thingDef>
		<countRange>750~1200</countRange>
	</li>
	<li Class="StockGenerator_SingleDef">
		<thingDef>WoodLog</thingDef>
		<countRange>100~350</countRange>
	</li>
	<li Class="StockGenerator_SingleDef">
		<thingDef>MedicineHerbal</thingDef>
		<countRange>20~50</countRange>
	</li>
	<li Class="StockGenerator_SingleDef">
		<thingDef>Beer</thingDef>
		<totalPriceRange>100~300</totalPriceRange>
	</li>
	<li Class="StockGenerator_SingleDef">
		<thingDef>Kibble</thingDef>
		<totalPriceRange>200~800</totalPriceRange>
	</li>

	<!-- Slaves -->
	<li Class="StockGenerator_Slaves">
		<respectPopulationIntent>true</respectPopulationIntent>
		<countRange>2~5</countRange>
	</li>

	<!-- Animals -->
	<li Class="StockGenerator_Animals">
		<tradeTagsSell>
			<li>AnimalFarm</li>
			<li>AnimalPet</li>
		</tradeTagsSell>
		<tradeTagsBuy>
			<li>AnimalUncommon</li>
			<li>AnimalExotic</li>
		</tradeTagsBuy>
		<kindCountRange>4~8</kindCountRange>
		<countRange>5~8</countRange>
		<createMatingPair>
			<li>AnimalFarm</li>
		</createMatingPair>
	</li>

<!-- Buying -->
	<li Class="StockGenerator_BuyExpensiveSimple" />
	<li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
	<li Class="StockGenerator_BuyTradeTag"><tag>MusicalInstrument</tag></li>
	<li Class="StockGenerator_BuyTradeTag"><tag>GHG_Apparel</tag></li>

	</stockGenerators>
</TraderKindDef>

</Defs>
