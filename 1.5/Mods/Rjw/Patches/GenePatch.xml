<?xml version="1.0" encoding="utf-8" ?>
<Patch>
<Operation Class="PatchOperationFindMod">
	<mods>
		<li>RJW Genes</li>
	</mods>
	<match Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationAdd">
				<xpath>Defs/AlienRace.ThingDef_AlienRace[defName = "Alien_Gokusotsu" or defName = "GHG_Mezu" or defName = "GHG_Gozu"]/alienRace/generalSettings</xpath>
				<value>
						<raceGenes>
							<li><defName>rjw_genes_homosexual</defName><chance>50</chance></li>
							<li><defName>rjw_genes_bisexual</defName><chance>90</chance></li>
							<li><defName>rjw_genes_futa</defName><chance>10</chance></li>
							<li><defName>rjw_genes_fertile_anus</defName><chance>10</chance></li>
						</raceGenes>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>Defs/AlienRace.ThingDef_AlienRace[defName = "Alien_Gokusotsu" or defName = "GHG_Gozu"]/alienRace/generalSettings/raceGenes</xpath>
				<value>
							<li><defName>rjw_genes_demonic_genitalia</defName></li>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>Defs/AlienRace.ThingDef_AlienRace[defName = "GHG_Mezu"]/alienRace/generalSettings/raceGenes</xpath>
				<value>
							<li><defName>rjw_genes_equine_genitalia</defName></li>
				</value>
			</li>
		</operations>
	</match>
</Operation>
</Patch>