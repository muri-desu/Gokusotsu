<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<ThingDef ParentName="PlantBase">
	<defName>Plant_IronSasa</defName>
	<label>iron bamboo grass</label>
	<description>A bamboo-like plant composed entirely of iron that grew wild as an undergrowth in the needle mountains in hell. Generally makes those who look at unsettling.</description>
	<descriptionHyperlinks>
		<ThingDef>Steel</ThingDef>
	</descriptionHyperlinks>
	<statBases>
		<MaxHitPoints>85</MaxHitPoints>
		<Flammability>0.2</Flammability>
		<Beauty>-2</Beauty>
		<BeautyOutdoors>-2</BeautyOutdoors>
	</statBases>

	<graphicData>
		<texPath>Gokusotsu/Things/Plant/IronSasa</texPath>
		<graphicClass>Graphic_Random</graphicClass>
		<renderQueue>1</renderQueue>
	</graphicData>

	<pathCost>60</pathCost>
	<altitudeLayer>Item</altitudeLayer>
	<selectable>true</selectable>
	<ingestible>
		<preferability>NeverForNutrition</preferability>
	</ingestible>

	<plant>
		<maxMeshCount>4</maxMeshCount>
		<fertilityMin>0.2</fertilityMin>
		<fertilitySensitivity>0.1</fertilitySensitivity>
		<harvestWork>300</harvestWork>
		<harvestedThingDef>Steel</harvestedThingDef>
		<burnedThingDef IsNull="True" />
		<harvestYield>2</harvestYield>
		<harvestTag>Standard</harvestTag>
		<sowTags>
			<li>Ground</li>
			<li>DecorativeTree</li>
		</sowTags>
		<topWindExposure>0</topWindExposure>
		<growMinGlow>0.3</growMinGlow>
		<growDays>6</growDays>
		<immatureGraphicPath>Gokusotsu/Things/Plant/IronSasa_Immature</immatureGraphicPath>
		<visualSizeRange>0.5~1.1</visualSizeRange>
	</plant>
</ThingDef>

</Defs>