<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- Skin Colors -->
	<GeneDef ParentName="GeneSkinColorOverride">
		<defName>GokusotsuSkin_Blue</defName>
		<label>deep blue skin</label>
		<labelShortAdj>blue</labelShortAdj>
		<description>Carriers of this gene produce a pigment that turns their skin a deep blue color.</description>
		<skinColorOverride>(80, 120, 160)</skinColorOverride>
		<displayOrderInCategory>89</displayOrderInCategory>
		<selectionWeight>0.8</selectionWeight>
		<symbolPack>
			<prefixSymbols>
				<li><symbol>blue</symbol></li>
			</prefixSymbols>
		</symbolPack>
	</GeneDef>
	
	<GeneDef ParentName="GeneSkinColorOverride">
		<defName>GokusotsuSkin_Green</defName>
		<label>deep green skin</label>
		<labelShortAdj>green</labelShortAdj>
		<description>Carriers of this gene produce a pigment that turns their skin a deep green color.</description>
		<skinColorOverride>(100, 160, 95)</skinColorOverride>
		<displayOrderInCategory>71</displayOrderInCategory>
		<symbolPack>
			<prefixSymbols>
				<li><symbol>green</symbol></li>
				<li><symbol>jade</symbol></li>
			</prefixSymbols>
		</symbolPack>
	</GeneDef>

