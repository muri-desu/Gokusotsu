<Defs>

<PawnKindDef Name="BaseGHGPawnKind" Abstract="True">
	<combatPower>60</combatPower>
	<isFighter>true</isFighter>
	<backstoryCryptosleepCommonality>0</backstoryCryptosleepCommonality>
	<initialWillRange>1~3</initialWillRange>
	<initialResistanceRange>10~20</initialResistanceRange>
	<itemQuality>Poor</itemQuality>
	<gearHealthRange>0.2~0.6</gearHealthRange>

	<xenotypeSet>
		<xenotypeChances>
			<XenoGokusotsu>999</XenoGokusotsu>
		</xenotypeChances>
		<warnIfTotalAbove1>false</warnIfTotalAbove1>
	</xenotypeSet>

</PawnKindDef>

